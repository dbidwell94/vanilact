{
  "name": "vanilact",
  "version": "1.0.45",
  "description": "A vanilla JS library that makes creating and nesting HTML Elements a breeze",
  "main": "js/index.js",
  "types": "js/index.d.ts",
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120,
    "quoteProps": "consistent"
  },
  "scripts": {
    "start": "webpack --watch",
    "build": "webpack --mode production",
    "pub": "npx @appnest/readme generate && git add . && git commit -m 'updated version and published' && git push && npm version patch && npm publish",
    "test": "parcel test/index.html",
    "push": "npx @appnest/readme generate && git add . && git commit -m 'wip' && git push",
    "readme": "npx @appnest/readme generate"
  },
  "keywords": [
    "dom",
    "react",
    "html",
    "typescript",
    "front-end",
    "react-dom",
    "es6"
  ],
  "author": "Devin Bidwell",
  "license": "MIT",
  "devDependencies": {
    "@appnest/readme": "^1.2.7",
    "parcel": "^2.0.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "repository": {
    "url": "git://github.com/dbidwell94/vanilact.git"
  }
}
